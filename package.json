{
  "name": "nuxt-2-web-app",
  "version": "1.0.0",
  "description": "Highclere Stud app",
  "engines": {
    "node": "12.13.0"
  },
  "author": "Elliot Alderton",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "heroku-postbuild": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/unit/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".+\\.(css|styl|less|sass|scss|png|jpg|svg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  },
  "dependencies": {
    "@bugsnag/js": "^6.4.3",
    "@bugsnag/plugin-express": "^6.4.1",
    "@bugsnag/plugin-vue": "^6.4.0",
    "@nuxtjs/axios": "^5.0.0",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "bugsnag-js": "^4.7.3",
    "connect-timeout": "^1.9.0",
    "cross-env": "^6.0.3",
    "debounce": "^1.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-http-proxy": "^1.4.0",
    "express-http-to-https": "^1.1.4",
    "express-rate-limit": "^5.0.0",
    "hammerjs": "^2.0.8",
    "helmet": "^3.15.0",
    "jest-cli": "^24.9.0",
    "js-cookie": "^2.2.0",
    "moment": "^2.22.2",
    "node-sass": "^4.13",
    "numeral": "^2.0.6",
    "nuxt": "^2.10.2",
    "pluralize": "^8.0.0",
    "sass-loader": "^8.0.0",
    "ssl-root-cas": "^1.3.1",
    "sweet-modal-vue": "^2.0.0",
    "underscore": "^1.9.1",
    "universal-cookie": "^4.0.2",
    "video.js": "^7.0.3",
    "vue": "^2.6.8",
    "vue-clickaway": "^2.2.2",
    "vue-scrollto": "^2.13.0",
    "vue-sweetalert2": "^2.1.5",
    "vue-template-compiler": "^2.6.8",
    "vuejs-datepicker": "^1.5.4",
    "vuelidate": "^0.7.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.1.0",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-storyshots": "^5.2.6",
    "@storybook/vue": "^5.2.6",
    "@vue/test-utils": "^1.0.0-beta.30",
    "babel-jest": "^24.9.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.1",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^15.2.1",
    "nodemon": "^2.0.1",
    "storybook-addon-vue-info": "^1.3.4",
    "style-loader": "^1.0.1",
    "vue-jest": "^3.0.5",
    "vue-server-renderer": "^2.5.17",
    "vue-svg-loader": "^0.11.0"
  }
}
